	<tr><td>ID</td><td>NOMBRE</td><td>EMAIL</td><td>ESTADO</td></tr>
	<%for(Profesor p: profesor){ %>
	<tr>
	<td><%= p.getIdProfesor()  %></td>
	<td><%= p.getName() %></td>
	<td><%= p.getEmail()%></td>
	<td><%= p.isEstado() %></td>
	</tr>
	<% }%>
<% }%>
