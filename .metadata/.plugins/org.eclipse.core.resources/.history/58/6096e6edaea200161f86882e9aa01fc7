package ulasalle;

public class Votante {
	String nombre;
	int dni;
	String direccion;
	String ciudad;
	String mesa;
	
	public Votante(String nombre,int dni,String direccion,String  mesa,String ciudad){
		this.nombre = nombre;
		this.dni = dni;
		this.direccion = direccion;
		this.mesa = mesa;
		this.ciudad = ciudad;
	}
		
	
}
